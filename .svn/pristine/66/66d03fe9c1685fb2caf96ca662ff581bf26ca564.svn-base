@import "common";
html,body{
    height: 100%;
}
.newWindowbackHeader{
    @extend %newWindowbackHeader;
    position: relative;
    z-index: 999;
    box-sizing: border-box;
}
.main_box{
    @extend %flex;
    @extend %flexCenter-x;
    height: 100%;
    position: relative;
    top:-1.173333rem;
    padding-top: 1.173333rem;
    box-sizing: border-box;
    .left{
        flex: 1;
        color: #555;
        @include font-dpr(14px);
        background-color: #f5f5f5;
        .item{
            box-sizing: border-box;
            border-left: 4px solid transparent;
            height: 1.04rem;
            line-height: 1.04rem;
            text-indent: 0.24rem;
            &.active{
                border-left-color:#3bce9d;
                color: #3bce9d;
                background-color: #fff;
            }
        }

    }
    .right{
        flex: 2;
        color: #333;
        @include font-dpr(14px);
        .item{
            @extend %flex;
            @extend %flexCenter-x;
            @extend %flexCenter-y;
            justify-content: space-between;
            padding: 0 0.32rem;
            box-sizing: border-box;
            height: 1.04rem;
            line-height: 1.04rem;
            border-bottom: 1px solid #e5e4e4;
            img{
                width: 0.2rem;
    			height: 0.4rem;
            }
        }
    }
}

//伤残鉴定详情样式
.level_box{
    .item{
        border-bottom: 1px solid #e5e4e4;
        @extend %flex;
        @extend %flexCenter-x;
        @extend %flexCenter-y;
        padding: 0.32rem;
        box-sizing: border-box;
        .level_title{
            width: 0.96rem;
            height: 0.48rem;
            border:1px solid #3bce9d;
            border-radius: 3px;
            color: #3bce9d;
            @include font-dpr(10px);
            text-align: center;
            @extend %flex;
            @extend %flexCenter-x;
            @extend %flexCenter-y;
            justify-content: center;
        }
        .level_text{
            color: #000;
            @include font-dpr(12px);
            margin-left: 0.32rem;
        }
    }

}
.level_group_btn{
    @extend %btngroup_1;
}
