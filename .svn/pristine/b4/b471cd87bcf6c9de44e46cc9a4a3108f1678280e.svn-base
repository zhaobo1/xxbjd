<script type="text/javascript">
var imageBrowser;
$(document).on('click','.photoitem',function(){
	var index = $(this).index();
	imageBrowser.openImages({
		activeIndex:index,
	    imageUrls: [
	        'widget://image/package.png',
	        'widget://image/package.png',
