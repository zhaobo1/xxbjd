@import "common";
html,body{
    height: 100%;
    background: #f4f4f4;
}
body{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
img{
    width: 1.066667rem;
    height: 1.066667rem;
}
div:nth-child(2){
    color: #24ca62;
    @include font-dpr(15px);
    margin:0.4rem 0 0.266667rem 0;
}
div:nth-child(3){
    color: #777;
    @include font-dpr(12px);
}
div:nth-child(4){
    color: #b5b5b5;
    @include font-dpr(10px);
    margin-top: 1.733333rem;
}
